<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="">

<resultMap id="UsuarioResult" type="">
</resultMap>

<insert id="registrarUsuario" parameterType="map">
	{call registrarUsuario(#{ID}, #{nombre}, #{mail}, #{username}, #{pass})}
</insert>

<insert id="iniciarSesion" parameterType="map">
	{call iniciarSesion(#{usuarioID}, #{passwd})}
</insert>

</mapper>